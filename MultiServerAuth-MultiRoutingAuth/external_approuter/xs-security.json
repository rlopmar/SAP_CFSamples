{
    "xsappname": "external_approuter_xsuaa",
    "tenant-mode": "dedicated",
    "description": "Security profile for: external_approuter",
    "scopes": [
        {
            "name": "$XSAPPNAME.routing_one_api",
            "description": "Allow routing to one_api server"
        },
        {
            "name": "$XSAPPNAME.routing_two_api",
            "description": "Allow routing to two_api server"
        }
    ],
    "attributes": [],
    "role-templates": [
        {
            "name": "central_routing_one_api",
            "description": "Allow routing to one_api server",
            "scope-references": ["$XSAPPNAME.routing_one_api"],
            "attribute-references": []
        },
        {
            "name": "central_routing_two_api",
            "description": "Allow routing to two_api server",
            "scope-references": ["$XSAPPNAME.routing_two_api"],
            "attribute-references": []
        }
    ],
    "oauth2-configuration": {
        "redirect-uris": [
            "https://*.eu10.applicationstudio.cloud.sap/**",
            "https://*.applicationstudio.cloud.sap/**",
            "https://*.cfapps.eu10.hana.ondemand.com/**",
            "https://*.eu10.hana.ondemand.com/**",
            "http://localhost:*/**"
        ]
    }
}
